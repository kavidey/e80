% all voltages are peak-to-peak
Vindiv = [1.53 1.48 1.48 1.48 1.48]';
Rin1 = 10070
Rin2 = 14760
Vin = Vindiv/(Rin2/(Rin1+Rin2));

Vout = [1.73 2.13 2.12 2.15 2.18]';
R2 = 67;
Rprobe = (Vin-Vout)*R2./Vout

saltMasses = [0.8 1.9 2.0 2.3 3.8]';
waterMasses = [81.1 56.6 58.4 60.6 89.7]';
salinity = saltMasses./(saltMasses+waterMasses);

scatter(salinity, Rprobe, "filled")
xlabel('Salinity (ppt)')
ylabel('Rprobe (\Omega)')
